{% import '_macros/atoms' as render %}

{%- minify -%}

    {% switch content.quoteType %}

        {%- case "pullQuote" -%}

            {{- render.atom('quote', 'pullquote', { 
                content: content.quote,
                position: content.position,
                source: content.quoteSource,
            }) -}}

        {%- case "blockQuote" -%}

            {{- render.atom('quote', 'blockquote', { 
                content: content.quote,
                source: content.quoteSource,
            }) -}}

    {% endswitch %}

{%- endminify -%}